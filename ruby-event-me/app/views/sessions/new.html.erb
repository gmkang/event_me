<h1>Log In</h1>

<%= form_for(:session, url: login_path) do |f| %>
<div class="login_form">
<%= f.label :email %> <br>
<%= f.email_field :email %> <br><br>
</div>
<div class ="login_form">
<%= f.label :password %> <br>
<%= f.password_field :password %> <br><br>
</div>
<%= f.submit("Log In") %>
<% end %>